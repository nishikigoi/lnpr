    "from scipy.stats import expon\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "zs = np.arange(0, 10, 0.01)\n",
    "ys = [expon.cdf(z, loc=0, scale=0.5) for z in zs]\n",
    "\n",
    "plt.plot(zs, ys)\n",
    "plt.show()"
   ]
  }
